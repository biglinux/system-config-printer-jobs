#!/bin/sh
set -e
package_name=system-config-printer-jobs
case "$1" in
  remove)
    dpkg-divert --package "$package_name" --rename --remove "/etc/xdg/autostart/print-applet.desktop"
    ;;
  purge)
    dpkg-divert --package "$package_name" --rename --remove "/etc/xdg/autostart/print-applet.desktop"
;;
esac
exit 0
